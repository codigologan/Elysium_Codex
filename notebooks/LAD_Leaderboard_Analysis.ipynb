    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "df = pd.read_csv(\"../reports/leaderboard.csv\")\n",
    "\n",
    "display(df)\n",
    "\n",
    "plt.figure(figsize=(6,4))\n",
    "plt.bar(df[\"run_name\"], df[\"val_loss\"])\n",
    "plt.ylabel(\"Validation Loss\")\n",
    "plt.title(\"LAD Leaderboard\")\n",
    "plt.show()\n"
   ]
